# Secrets and API Keys Template
# Copy this file to .zshrc.secrets and fill in your actual keys

# GitHub
# Get from: https://github.com/settings/tokens
export GITHUB_TOKEN=your_github_token_here

# AI Agents Keys

## Gemini
# Get from: https://makersuite.google.com/app/apikey
export GEMINI_API_KEY=your_gemini_api_key_here
export GOOGLE_GENERATIVE_AI_API_KEY=$GEMINI_API_KEY

## OpenAI
# Get from: https://platform.openai.com/api-keys
export OPENAI_API_KEY=your_openai_api_key_here

# Keycloak
export KC_BASE_URL=your_keycloak_base_url_here
export KC_REALM=your_realm_here
export KC_CLIENT_ID=your_client_uuid_here
export KC_ADMIN_CLIENT_ID=your_admin_client_id_here
export KC_ADMIN_CLIENT_SECRET=your_client_secret_here
