{
  "statusLine": {
    "type": "command",
    "command": "~/.claude/statusline.sh",
    "padding": 0
  },
  "cleanupPeriodDays": 5,
  "includeCoAuthoredBy": false,
  "model": "opus",
  "alwaysThinkingEnabled": true,
  "gitAttribution": false,
  "permissions": {
    "ask": [
      "Bash(git commit:*)"
    ],
    "deny": [
      "Bash(git push:*)"
    ]
  }
}
